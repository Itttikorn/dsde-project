content/abstract/scopus_id/84907575011", "dc:creator": {"author": [{"ce:given-name": "Supakit", "preferred-name": {"ce:given-name": "Supakit", "ce:initials": "S.", "ce:surname": "Seawpakorn", "ce:indexed-name": "Seawpakorn S."}, "@seq": "1", "ce:initials": "S.", "@_fa": "true", "affiliation": {"@id": "60028190", "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60028190"}, "ce:surname": "Seawpakorn", "@auid": "56066536700", "author-url": "https://api.elsevier.com/content/author/author_id/56066536700", "ce:indexed-name": "Seawpakorn S."}]}, "link": [{"@_fa": "true", "@rel": "self", "@href": "https://api.elsevier.com/content/abstract/scopus_id/84907575011"}, {"@_fa": "true", "@rel": "scopus", "@href": "https://www.scopus.com/inward/record.uri?partnerID=HzOxMe3b&scp=84907575011&origin=inward"}, {"@_fa": "true", "@rel": "scopus-citedby", "@href": "https://www.scopus.com/inward/citedby.uri?partnerID=HzOxMe3b&scp=84907575011&origin=inward"}], "source-id": "21960", "citedby-count": "0", "prism:volume": "26", "subtype": "ar", "dc:title": "Picolinic diamide pincers for transition metal complexation and their electrochemical properties", "openaccess": "0", "prism:issn": "10290478 10610278", "publishercopyright": "\u00a9 2014 Taylor & Francis.", "prism:issueIdentifier": "10-12", "subtypeDescription": "Article", "prism:publicationName": "Supramolecular Chemistry", "prism:pageRange": "843-849", "prism:endingPage": "849", "openaccessFlag": "false", "prism:doi": "10.1080/10610278.2014.883076", "prism:startingPage": "843", "dc:identifier": "SCOPUS_ID:84907575011", "dc:publisher": "Taylor and Francis Ltd."}, "idxterms": null, "language": {"@xml:lang": "eng"}, "authkeywords": null, "subject-areas": {"subject-area": [{"@_fa": "true", "$": "Chemistry (all)", "@code": "1600", "@abbrev": "CHEM"}]}, "authors": {"author": [{"ce:given-name": "Supakit", "preferred-name": {"ce:given-name": "Supakit", "ce:initials": "S.", "ce:surname": "Seawpakorn", "ce:indexed-name": "Seaw