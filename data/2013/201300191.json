", "ce:initials": "G.", "@_fa": "true", "affiliation": {"@id": "60028333", "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60028333"}, "ce:surname": "Srzednicki", "@auid": "55949009900", "author-url": "https://api.elsevier.com/content/author/author_id/55949009900", "ce:indexed-name": "Srzednicki G."}, {"preferred-name": {"ce:given-name": "C.", "ce:initials": "C.", "ce:surname": "Borompichaichartkul", "ce:indexed-name": "Borompichaichartkul C."}, "@seq": "3", "ce:initials": "C.", "@_fa": "true", "affiliation": {"@id": "60028190", "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60028190"}, "ce:surname": "Borompichaichartkul", "@auid": "8242484100", "author-url": "https://api.elsevier.com/content/author/author_id/8242484100", "ce:indexed-name": "Borompichaichartkul C."}, {"preferred-name": {"ce:given-name": "P.", "ce:initials": "P.", "ce:surname": "Vaithanomsat", "ce:indexed-name": "Vaithanomsat P."}, "@seq": "4", "ce:initials": "P.", "@_fa": "true", "affiliation": {"@id": "60021944", "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60021944"}, "ce:surname": "Vaithanomsat", "@auid": "16041042600", "author-url": "https://api.elsevier.com/content/author/author_id/16041042600", "ce:indexed-name": "Vaithanomsat P."}]}}}