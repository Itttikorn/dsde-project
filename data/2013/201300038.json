onyear": {"@first": "1985"}, "refd-itemidlist": {"itemid": {"$": "0000390291", "@idtype": "SGR"}}, "ref-volisspag": {"voliss": {"@volume": "18"}, "pagerange": {"@first": "1436"}}, "ref-authors": {"author": [{"@seq": "1", "ce:initials": "H.", "@_fa": "true", "ce:surname": "Watanabe", "ce:indexed-name": "Watanabe H."}, {"@seq": "2", "ce:initials": "T.", "@_fa": "true", "ce:surname": "Sakamoto", "ce:indexed-name": "Sakamoto T."}, {"@seq": "3", "ce:initials": "T.", "@_fa": "true", "ce:surname": "Kotaka", "ce:indexed-name": "Kotaka T."}]}, "ref-sourcetitle": "Macromolecules"}}]}}}}, "affiliation": [{"affiliation-city": "Bangkok", "@id": "60028190", "affilname": "Chulalongkorn University", "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60028190", "affiliation-country": "Thailand"}, {"affiliation-city": "Nakhon Ratchasima", "@id": "60026326", "affilname": "Suranaree University of Technology", "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60026326", "affiliation-country": "Thailand"}, {"affiliation-city": "Pitsanulok", "@id": "60022498", "affilname": "Naresuan University", "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60022498", "affiliation-country": "Thailand"}, {"affiliation-city": "Nakhon Pathom", "@id": "60012718", "affilname": "Mahidol University", "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60012718", "affiliation-country": "Thailand"}], "coredata": {"srctype": "j", "eid": "2-s2.0-84887486898", "dc:description": "This study introduces a new class of materials as a dewetting inhibitor for polystyrene (PS) ultrathin films. Two types of highly branched aromatic (HBA) molecules are added into PS films with thicknesses of 7 nm and 23 nm. Their concentrations range from 0.75 to 5 wt.%. The films are annealed in vacuum oven at elevated temperatures to accelerate dewetting process. Evolution of the film morphologies is followed by utilizing atomic force microscopy and optical microscopy. Contact angle measurements are used to evaluate interfacial interactions in each system. Dewetting area as a function of annealing time and HBA concentration are calculated. We have found that the presence of only 0.5 wt.% HBA can suppress the dewetting dynamics of PS films. Increasing the HBA concentration from 0.5 to 5 wt.% causes systematic decrease of the dewetting rate. In this system, the HBA molecules behave as physical cross-linking points for PS chains, which lead to the improvement of film stability. The efficiency of HBA as a dewetting inhibitor varies with molecular weight of PS while the change of HBA structure hardly affects the dewetting behaviors. \u00a9 2013 Elsevier B.V. All rights reserved.", "prism:coverDate": "2013-12-02", "prism:aggregationType": "Journal", "prism:url": "https://api.elsevier.com/content/abstract/scopus_id/84887486898", "subtypeDescription": "Article", "dc:creator": {"author": [{"ce:given-name": "Nampueng", "preferred-name": {"ce:given-name": "Nampueng", "ce:initials": "N.", "ce:surname": "Pangpaiboon", "ce:indexed-name": "Pangpaiboon N."}, "@seq": "1", "ce:initials": "N.", "@_fa": "true", "affiliation": {"@id": "60028190", "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60028190"}, "ce:surname": "Pangpaiboon", "@auid": "55867328200", "author-url": "https://api.elsevier.com/content/author/author_id/55867328200", "ce:indexed-name": "Pangpaiboon N."}]}, "link": [{"@_fa": "true", "@rel": "self", "@href": "https://api.elsevier.com/content/abstract/scopus_id/84887486898"}, {"@_fa": "true", "@rel": "scopus", "@href": "https://www.scopus.com/inward/record.uri?partnerID=HzOxMe3b&scp=84887486898&origin=inward"}, {"@_fa": "true", "@rel": "scopus-citedby", "@href": "https://www.scopus.com/inward/citedby.uri?partnerID=HzOxMe3b&scp=84887486898&origin=inward"}], "prism:publicationName": "Thin Solid Films", "source-id": "12347", "pii": "S0040609013015319", "citedby-count": "6", "prism:volume": "548", "subtype": "ar", "prism:pageRange": "323-330", "dc:title": "Retardation the dewetting dynamics of ultrathin polystyrene films using highly branched aromatic molecules as additives", "prism:endingPage": "330", "openaccess": "0", "openaccessFlag": "false", "prism:doi": "10.1016/j.tsf.2013.09.059", "prism:issn": "00406090", "prism:startingPage": "323", "dc:identifier": "SCOPUS_ID:84887486898"}, "idxterms": {"mainterm": [{"$": "Aromatic molecules", "@weight": "a", "@candidate": "n"}, {"$": "De-wetting", "@weight": "a", "@candidate": "n"}, {"$": "Dewetting process", "@weight": "a", "@candidate": "n"}, {"$": "Elevated temperature", "@weight": "a", "@candidate": "n"}, {"$": "Interfacial interaction", "@weight": "a", "@candidate": "n"}, {"$": "Physical cross-linking", "@weight": "a", "@candidate": "n"}, {"$": "Polymeric thin-films", "@weight": "a", "@candidate": "n"}, {"$": "Polystyrene films", "@weight": "a", "@candidate": "n"}]}, "language": {"@xml:lang": "eng"}, "authkeywords": {"author-keyword": [{"@_fa": "true", "$": "Additive"}, {"@_fa": "true", "$": "Dewetting inhibition"}, {"@_fa": "true", "$": "Physical cross-linking"}, {"@_fa": "true", "$": "Polymeric thin films"}, {"@_fa": "true", "$": "Polystyrene"}]}, "subject-areas": {"subject-area": [{"@_fa": "true", "$": "Electronic, Optical and Magnetic Materials", "@code": "2504", "@abbrev": "MATE"}, {"@_fa": "true", "$": "Surfaces and Interfaces", "@code": "3110", "@abbrev": "PHYS"}, {"@_fa": "true", "$": "Surfaces, Coatings and Films", "@code": "2508", "@abbrev": "MATE"}, {"@_fa": "true", "$": "Metals and Alloys", "@code": "2506", "@abbrev": "MATE"}, {"@_fa": "true", "$": "Materials Chemistry", "@code": "2505", "@abbrev": "MATE"}]}, "authors": {"author": [{"ce:given-name": "Nampueng", "preferred-name": {"ce:given-name": "Nampueng", "ce:initials": "N.", "ce:surname": "Pangpaiboon", "ce:indexed-name": "Pangpaiboon N."}, "@seq": "1", "ce:initials": "N.", "@_fa": "true", "affiliation": {"@id": "60028190", "@href": "https://api.elsevier.