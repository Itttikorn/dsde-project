ntry": "tha", "city": "Bangkok", "organization": [{"$": "Excellent Center of Organ Transplantation"}, {"$": "King Chulalongkorn Memorial Hospital"}, {"$": "Thai Red Cross Society"}], "affiliation-id": [{"@afid": "60022183", "@dptid": "114274357"}, {"@afid": "60031780"}], "@dptid": "114274357"}, "author": [{"ce:given-name": "Aekarach", "preferred-name": {"ce:given-name": "Aekarach", "ce:initials": "A.", "ce:surname": "Ariyachaipanich", "ce:indexed-name": "Ariyachaipanich A."}, "@seq": "4", "ce:initials": "A.", "@_fa": "true", "@type": "auth", "ce:surname": "Ariyachaipanich", "@auid": "35208073900", "ce:indexed-name": "Ariyachaipanich A."}, {"ce:given-name": "Supaporn", "preferred-name": {"ce:given-name": "Supaporn", "ce:initials": "S.", "ce:surname": "Sritangsirikul", "ce:indexed-name": "Sritangsirikul S."}, "@seq": "6", "ce:initials": "S.", "@_fa": "true", "@type": "auth", "ce:surname": "Sritangsirikul", "@auid": "25123319800", "ce:indexed-name": "Sritangsirikul S."}, {"ce:given-name": "Kanokwan", "preferred-name": {"ce:given-name": "Kanokwan", "ce:initials": "K.", "ce:surname": "Thammanatsakul", "ce:indexed-name": "Thammanatsakul K."}, "@seq": "7", "ce:initials": "K.", "@_fa": "true", "@type": "auth", "ce:surname": "Thammanatsakul", "@auid": "57196410110", "ce:indexed-name": "Thammanatsakul K."}]}], "citation-title": "Low-dose basiliximab induction therapy in heart transplantation", "abstracts": "\u00a9 2017 John Wiley & Sons A/S. Published by John Wiley & Sons LtdWe prospectively studied efficacy and safety outcomes of two 10-mg doses of intravenous basiliximab on day 0 and day 4 for induction therapy in 17 consecutive de novo heart transplant recipients. By the 2-week assessment post-transplant, there were no deaths, graft failures, or acute cellular rejections (ACRs) ISHLT grade \u2265 2R. By the 1-year assessment post-transplant, there were 1 (6%) infectious death, no graft failures, 2 (12%) grade 2R ACRs, 6 (35%) asymptomatic cytomegalovirus (CMV) infections, and 4 (25%) treated infections. Our study was the first to show that low-dose basiliximab induction in heart transplant resulted in favorable efficacy and safety outcomes. Additionally, calcineurin inhibitor (CNI) initiation in a low-risk population could be safely delayed using the strategy of modified low-dose postoperative basiliximab. This strategy also appears to allow subsequent early corticosteroid wean, although with the concomitant maintenance of higher CNI levels and higher dosing of mycophenolate.", "correspondence": {"affiliation": {"country": "Thailand", "@country": "tha", "city": "Bangkok", "organization": [{"$": "Division of Cardiovascular Medicine"}, {"$": "Department of Medicine"}, {"$": "Faculty of Medicine"}, {"$": "Chulalongkorn University"}]}, "person": {"ce:given-name": "Sarinya", "ce:initials": "S.", "ce:surname": "Puwanant", "ce:indexed-name": "Puwanant S."}}, "citation-info": {"author-keywords": {"author-keyword": [{"$": "fusion proteins and monoclonal antibodies: basiliximab/daclizumab", "@xml:lang": "eng"}, {"$": "heart (allograft) function/dysfunction", "@xml:lang": "eng"}, {"$": "immunosuppressant", "@xml:lang": "eng"}, {"$": "rejection", "@xml:lang": "eng"}]}, "citation-type": {"@code": "ar"}, "citation-language": {"@language": "English", "@xml:lang": "eng"}, "abstract-language": {"@language": "English", "@xml:lang": "eng"}}, "source": {"website": {"ce:e-address": {"$": "http://www.wiley.com/bw/journal.asp?ref=0902-0063", "@type": "email"}}, "translated-sourcetitle": {"$": "Clinical Transplantation", "@xml:lang": "eng"}, "volisspag": {"voliss": {"@volume": "31", "@issue": "12"}}, "@type": "j", "sourcetitle": "Clinical Transplantation", "publicationdate": {"month": "12", "year": "2017", "date-text": "December 2017", "day": "01"}, "codencode": "CLTRE", "sourcetitle-abbrev": "Clin. Transplant.", "@country": "gbr", "issn": [{"$": "13990012", "@type": "electronic"}, {"$": "09020063", "@type": "print"}], "publicationyear": {"@first": "2017"},