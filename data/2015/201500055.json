liation-id": {"@afid": "60028190", "@dptid": "110122742"}, "@dptid": "110122742"}, "author": [{"ce:given-name": "Siyada", "preferred-name": {"ce:given-name": "Siyada", "ce:initials": "S.", "ce:surname": "Jaiyen", "ce:indexed-name": "Jaiyen S."}, "@seq": "2", "ce:initials": "S.", "@_fa": "true", "@type": "auth", "ce:surname": "Jaiyen", "@auid": "55565953800", "ce:indexed-name": "Jaiyen S."}, {"ce:given-name": "Chawalit", "preferred-name": {"ce:given-name": "Chawalit", "ce:initials": "C.", "ce:surname": "Ngamcharussrivichai", "ce:indexed-name": "Ngamcharussrivichai C."}, "@seq": "3", "ce:initials": "C.", "@_fa": "true", "@type": "auth", "ce:surname": "Ngamcharussrivichai", "@auid": "21734976900", "ce:indexed-name": "Ngamcharussrivichai C."}]}, {"affiliation": {"country": "Thailand", "@afid": "60028190", "@country": "tha", "city": "Patumwan,Bangkok", "organization": [{"$": "Center of Excellence on Petrochemical and Materials Technology (PETROMAT)"}, {"$": "Chulalongkorn University"}], "affiliation-id": [{"@afid": "60028190"}, {"@afid": "60199582"}]}, "author": [{"ce:given-name": "Chawalit", "preferred-name": {"ce:given-name": "Chawalit", "ce:initials": "C.", "ce:surname": "Ngamcharussrivichai", "ce:indexed-name": "Ngamcharussrivichai C."}, "@seq": "3", "ce:initials": "C.", "@_fa": "true", "@type": "auth", "ce:surname": "Ngamcharussrivichai", "@auid": "21734976900", "ce:indexed-name": "Ngamcharussrivichai C."}]}], "citation-title": "Al2O3-supported Mixed Ca and Zn Compounds Prepared from Waste Seashells for Synthesis of Palm Fatty Acid Methyl Esters", "abstracts": "\u00a9 2015, Copyright \u00a9 Taylor & Francis Group, LLC.Waste mixed seashells of Perna viridis, Anadara granosa, Amusium pleuronectes, and Meretrix meretrix, as abundant, low-cost, and nontoxic calcium sources, were used as renewable raw materials in the preparation of a heterogeneous base catalyst for the transesterification of palm oil in refluxed methanol. The new mixed metal compound catalyst was prepared by mixing the calcined seashells with Zn(NO3)2 and Al2O3 in an acidic aqueous solution, followed by calcination at 500\u00b0C (ZSA-500). The ZSA-500 catalyst exhibited an enhanced surface area, dispersion, and total basicity compared to the parent calcined seashells. The fatty acid methyl ester (FAME) yield attained over ZSA-500 was 99 wt.% and dropped by 3% after five repetitive uses when the reaction was performed for 3 h at 60\u00b0C and ambient pressure with 10 wt.% catalyst and a methanol:oil molar ratio of 30:1. Structural and thermal analysis indicated that the active phases of ZSA-500 had a high stability against the glycerol adsorption as well as the phase transformation to calcium glyceroxides (Ca(C3H7O3)2).", "correspondence": {"affiliation": {"country": "Thailand", "postal-code": "10330", "@country": "tha", "city": "Patumwan,Bangkok", "organization": [{"$": "Department of Chemical Technology"}, {"$": "Chulalongkorn University"}]}, "person": {"ce:given-name": "Chawalit", "ce:initials": "C.", "ce:surname": "Ngamcharussrivichai", "ce:indexed-name": "Ngamcharussrivichai C."}}, "citation-info": {"author-keywords": {"author-keyword": [{"$": "Biodiesel", "@xml:lang": "eng"}, {"$": "Calcium oxide", "@xml:lang": "eng"}, {"$": "Mixed metal oxides", "@xml:lang": "eng"}, {"$": "Transesterification", "@xml:lang": "eng"}, {"$": "Waste seashells", "@xml:lang": "eng"}]}, "citation-type": {"@code": "ar"}, "citation-language": {"@language": "English", "@xml:lang": "eng"}, "abstract-language": {"@language": "English", "@xml:lang": "eng"}}, "source": {"website": {"ce:e-address": {"$": "www.tandf.co.uk/journals/titles/00986445.asp", "@type": "email"}}, "translated-sourcetitle": {"$": "Chemical Engineering Communications", "@xml:lang": "eng"}, "volisspag": {"voliss": {"@volume": "202", "@issue": "12"}, "pagerange": {"@first": "1591", "@last": "1599"}}, "@type": "j", "sourcetitle": "Chemical Engineering Communications", "publicationdate": {"month": "12", "year": "2015", "da