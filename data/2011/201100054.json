ator": {"author": [{"ce:given-name": "Pisitpong", "preferred-name": {"ce:given-name": "Pisitpong", "ce:initials": "P.", "ce:surname": "Intarapong", "ce:indexed-name": "Intarapong P."}, "@seq": "1", "ce:initials": "P.", "@_fa": "true", "affiliation": {"@id": "60028190", "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60028190"}, "ce:surname": "Intarapong", "@auid": "24485470400", "author-url": "https://api.elsevier.com/content/author/author_id/24485470400", "ce:indexed-name": "Intarapong P."}]}, "link": [{"@_fa": "true", "@rel": "self", "@href": "https://api.elsevier.com/content/abstract/scopus_id/84878371554"}, {"@_fa": "true", "@rel": "scopus", "@href": "https://www.scopus.com/inward/record.uri?partnerID=HzOxMe3b&scp=84878371554&origin=inward"}, {"@_fa": "true", "@rel": "scopus-citedby", "@href": "https://www.scopus.com/inward/citedby.uri?partnerID=HzOxMe3b&scp=84878371554&origin=inward"}], "prism:publicationName": "International Journal of Renewable Energy Research", "source-id": "21100258747", "citedby-count": "20", "prism:volume": "1", "subtype": "ar", "prism:pageRange": "271-280", "dc:title": "Transesterification of palm oil over KOH/NaY zeolite in a packed-bed reactor", "prism:endingPage": "280", "openaccess": null, "openaccessFlag": null, "prism:issn": "13090127", "prism:startingPage": "271", "dc:identifier": "SCOPUS_ID:84878371554"}, "idxterms": null, "language": {"@xml:lang": "eng"}, "authkeywords": {"author-keyword": [{"@_fa": "true", "$": "Biodiesel"}, {"@_fa": "true", "$": "Leaching."}, {"@_fa": "true", "$": "NaY"}, {"@_fa": "true", "$": "Solid catalyst"}, {"@_fa": "true", "$": "Transesterification"}]}, "subject-areas": {"subject-area": [{"@_fa": "true", "$": "Renewable Energy, Sustainability and the Environment", "@code": "2105", "@abbrev": "ENER"}, {"@_fa": "true", "$": "Energy Engineering and Power Technology", "@code": "2102", "@abbrev": "ENER"}]}, "authors": {"author": [{"ce:given-name": "Pisitpong", "preferred-name": {"ce:give